{
    "builds": [
      {
        "src": "src/main.py",
        "use": "@vercel/python",
        "config": {
          "installCommand": "pip install -r requirements.txt && playwright install chromium",
          "includeFiles": ["src/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.py"
      }
    ]
}